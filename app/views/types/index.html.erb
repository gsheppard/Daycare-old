<h1>Listing types</h1>

<div class="col-md-4">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>

    <tbody>
      <% @types.each do |type| %>
        <tr>
          <td><%= link_to type.identifier.capitalize, type %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
